# Server configuration
PORT=3000

# NEAR Network configuration
NEAR_NETWORK=testnet
NEAR_NODE_URL=https://rpc.testnet.near.org
NEAR_WALLET_URL=https://wallet.testnet.near.org
NEAR_HELPER_URL=https://helper.testnet.near.org
NEAR_EXPLORER_URL=https://explorer.testnet.near.org

# NEAR Account configuration
# You will need to set up a NEAR account and add the private key here
# These credentials are used for both blockchain transactions and NEAR AI API authentication
NEAR_ACCOUNT_ID=your-account.testnet
NEAR_PRIVATE_KEY=ed25519:your-private-key

# NEAR AI configuration
# Set USE_ACTUAL_API=true to use the actual NEAR AI API
USE_ACTUAL_API=true
NEAR_AI_API_URL=https://api.near.ai
# You can provide a pre-generated auth token instead of using NEAR account credentials
# If both NEAR_ACCOUNT_ID/NEAR_PRIVATE_KEY and NEAR_AI_AUTH_TOKEN are provided,
# the NEAR account credentials will be used to generate a fresh token
NEAR_AI_AUTH_TOKEN=your-near-ai-auth-token
# The model to use for AI operations
NEAR_AI_MODEL=near-ai-agent-v1

# Legacy configuration (deprecated)
# These are maintained for backwards compatibility
AI_SERVICE_ENDPOINT=https://api.example.com/ai
AI_SERVICE_API_KEY=your-api-key 